# module to make electronPixelSeeds via SubSeedGenerator
module electronPixelSeedsForGlobalGsfElectrons=GlobalSeedProducer {
 InputTag barrelSuperClusters = correctedHybridSuperClusters
 InputTag endcapSuperClusters = correctedIslandEndcapSuperClusters
 InputTag initialSeeds=hltL1IsoEgammaRegionalPixelSeedGenerator
    double seedPt              = 0.
    double seedDr              = 0.3
    double seedDPhi            = 0.1
    double seedDEta            = 0.025
}
